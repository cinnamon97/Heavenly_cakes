#: .\env\Lib\site-packages\django\contrib\postgres\apps.py:54
msgid "PostgreSQL extensions"
msgstr "PostgreSQL-laajennukset"

#: .\env\Lib\site-packages\django\contrib\postgres\fields\array.py:21
#: .\env\Lib\site-packages\django\contrib\postgres\forms\array.py:17
#: .\env\Lib\site-packages\django\contrib\postgres\forms\array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Taulukon %(nth)s alkio ei ole kelvollinen:"

#: .\env\Lib\site-packages\django\contrib\postgres\fields\array.py:22
msgid "Nested arrays must have the same length."
msgstr "Sisäkkäisillä taulukoilla tulee olla sama pituus."

#: .\env\Lib\site-packages\django\contrib\postgres\fields\hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Kartta merkkijonoista merkkijonoihin tai tyhjiin (null)"

#: .\env\Lib\site-packages\django\contrib\postgres\fields\hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Avaimen \"%(key)s\" arvo ei ole merkkijono tai tyhjä (null)."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\hstore.py:17
msgid "Could not load JSON data."
msgstr "JSON-dataa ei voitu ladata."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "Syötteen tulee olla JSON-sanakirja."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:42
msgid "Enter two valid values."
msgstr "Anna kaksi oikeaa arvoa."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "Alueen alku pitää olla pienempi kuin alueen loppu."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:99
msgid "Enter two whole numbers."
msgstr "Anna kaksi kokonaislukua."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:105
msgid "Enter two numbers."
msgstr "Anna kaksi lukua."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:111
msgid "Enter two valid date/times."
msgstr "Anna kaksi oikeaa päivämäärää/kellonaikaa."

#: .\env\Lib\site-packages\django\contrib\postgres\forms\ranges.py:117
msgid "Enter two valid dates."
msgstr "Anna kaksi oikeaa päivämäärää."

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Listassa on %(show_value)d arvo, mutta siinä ei saa olla enempää kuin "
"%(limit_value)d."
msgstr[1] ""
"Listassa on %(show_value)d arvoa, mutta siinä ei saa olla enempää kuin "
"%(limit_value)d."

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Listassa on %(show_value)d arvo, mutta siinä ei saa olla vähempää kuin "
"%(limit_value)d arvoa."
msgstr[1] ""
"Listassa on %(show_value)d arvoa, mutta siinä ei saa olla vähempää kuin "
"%(limit_value)d."

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Joitain avaimia puuttuu: %(keys)s"

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Tuntemattomia avaimia annettiin: %(keys)s"

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:81
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely greater than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr ""
"Tämän alueen tulee olla kokonaisuudessaan yhtäsuuri tai suurempi kuin "
"%(limit_value)s."

#: .\env\Lib\site-packages\django\contrib\postgres\validators.py:90
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely less than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr ""
"Tämän alueen tulee olla kokonaisuudessaan yhtäsuuri tai pienempi kuin "
"%(limit_value)s."
